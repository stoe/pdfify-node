{
  "name": "pdfify-node",
  "version": "2.2.0",
  "description": "Convert markdown files to PDF.",
  "keywords": [],
  "author": {
    "name": "Stefan StÃ¶lzle",
    "email": "stefan@github.com",
    "url": "stefan.stoelzle.me"
  },
  "license": "MIT",
  "homepage": "https://github.com/stoe/pdfify-node#readme",
  "repository": "stoe/pdfify-node",
  "bugs": {
    "url": "https://github.com/stoe/pdfify-node/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "bin": {
    "pdfify": "./index.js"
  },
  "main": "index.js",
  "scripts": {
    "test": "xo && ava --verbose",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "scss-compile": "node-sass --include-path scss --output-style compressed ./scss/style.scss ./assets/style.css",
    "postinstall": "npm run scss-compile"
  },
  "dependencies": {
    "bluebird": "latest",
    "chalk": "latest",
    "gh-got": "latest",
    "handlebars": "latest",
    "highlight.js": "latest",
    "html-pdf": "latest",
    "markdown-it": "latest",
    "markdown-it-emoji": "latest",
    "markdown-it-highlightjs": "latest",
    "meow": "latest",
    "node-sass": "latest",
    "opn": "latest",
    "ora": "latest",
    "write-json-file": "latest"
  },
  "devDependencies": {
    "ava": "latest",
    "eslint": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-node": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-standard": "latest",
    "execa": "latest",
    "snyk": "latest",
    "xo": "latest"
  },
  "ava": {
    "files": [
      "test/*.t.js"
    ]
  },
  "xo": {
    "space": 2,
    "ignores": [
      "assets/"
    ]
  },
  "snyk": true
}
